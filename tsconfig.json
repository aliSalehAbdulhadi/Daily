{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/assets/svgExport/arrow.js,",
    "src/redux/store/store.ts",
    "src/components/Navbar/Navbar.tsx"
, "src/components/swipeable/Swipeable.jsx", "src/redux/slices/features/disableDrag.js"  ],
  "exclude": ["node_modules"]
}
